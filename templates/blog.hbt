{{> header}}

<h2>{{title}}</h2>

<article>
    <ul>
        {{#each collections.posts}}
            <li>
                <h3><a href="{{link this.path}}">{{ this.title }}</a></h3>
                <article>{{{ this.contents }}}</article>
            </li>
        {{/each}}
    </ul>
</article>

{{> footer}}